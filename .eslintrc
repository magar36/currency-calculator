{
"extends": "airbnb",
"parserOptions": {
  "ecmaVersion": 6
  },
"env": {
  "node": true,
  "browser": true,
  "es6": true
  },
"rules": {
  "comma-dangle": ["warn", "never"],
  "no-multiply-empty-lines": "never",
  "no-console": ["error", { allow: ["log"] }],
  "no-multiple-empty-lines": ["error", { "max": 2 }]
  }
}
